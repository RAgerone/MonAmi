option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "monami/monami.settings"
    PYTHONPATH: "$PYTHONPATH"

container_commands:
  01_migrate:
    command: "python3 monami/manage.py migrate"
    leader_only: true
  02_collectstatic:
    command: "python3 monami/manage.py collectstatic --noinput"
